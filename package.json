{
  "name": "socket-set",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "setup": "cd client && yarn install",
    "build": "cd client && npm run build",
    "client": "cd vite-client && npm run dev",
    "dev": "concurrently \"npm run client\" \"npm run server:dev\"",
    "dev:db": "cd db; docker-compose up -d",
    "server:dev": "nodemon server.js",
    "predeploy": "cd vite-client && npm run build",
    "deploy": "gh-pages -d vite-client/dist -b rp-pages -e client/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cookie-session": "^1.3.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "knex": "^3.1.0",
    "morgan": "^1.9.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "gh-pages": "^6.3.0",
    "husky": "^4.2.1",
    "nodemon": "^3.1.10",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
