{
  "name": "socket-set",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "setup": "cd client && yarn install",
    "build": "cd client && npm run build",
    "dev": "concurrently \"npm run start --prefix client\" \"npm run server:dev\"",
    "server": "dev_appserver.py .",
    "dev:db": "cd devdb; docker-compose up -d",
    "server:flask": "bash ./run-py.sh",
    "server:dev": "cross-env NODE_ENV=local nodemon server.js",
    "predeploy": "cd client && npm run build",
    "deploy": "gh-pages -d client/build -b rp-pages -e client/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^7.0.0",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.3.3",
    "cross-env": "^7.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "gh-pages": "^2.2.0",
    "knex": "^0.95.15",
    "mariadb": "^2.1.3",
    "morgan": "^1.9.1",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.15",
    "react-bootstrap": "^1.0.0-beta.16",
    "sequelize": "^5.21.2",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "husky": "^4.2.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
